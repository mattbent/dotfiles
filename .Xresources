URxvt.perl-ext-common: resize-font
URxvt.scrollBar: false
URxvt.font: xft:Hack:pixelsize=11:Regular
URxvt.boldFont: xft:Hack:pixelsize=11:Bold
URxvt.italicFont: xft:Hack:pixelsize=11:Italic
URxvt.boldItalicFont: xft:Hack:pixelsize=11:Bold:Italic
URxvt.resize-font.smaller: Shift-Down
URxvt.resize-font.bigger: Shift-Up
